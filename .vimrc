colorscheme desert
syntax on
set number
set enc=utf-8
set fenc=utf-8
set termencoding=utf-8
set nocompatible
set autoindent
set smartindent
set tabstop=4
set shiftwidth=4
set expandtab
set textwidth=120
set showmatch
set comments=sl:/*,mb:\ *,elx:\ */
filetype on
set cursorline
set cursorcolumn
set showcmd
set guifont=Monospace\ Regular\ 14
set lines=40
set columns=120
